@model ContactModel

@{
    ViewData["Title"] = "Delete contact";
}
<div class="text-center mb-5">
    <h3>Really want to delete @Model.Name?</h3>
</div>

<div class="text-center ">
    <input type="hidden" asp-for="ID" />

<a class="btn btn-primary text-white" role="button" asp-controller="Contact" asp-action="Index">Cancel exclude</a>
    <a class="btn btn-danger text-white" role="button" asp-controller="Contact" asp-route-id="@Model.ID" asp-action="Delete">Confirm exclude</a>
</div>



